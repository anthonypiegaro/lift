{
  "expo": {
    "name": "lift",
    "slug": "lift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lift_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/lift_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anthonypiegaro.lift"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lift_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/lift_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "08c6716b-eca1-4beb-af35-6598379c6180"
      }
    }
  }
}
